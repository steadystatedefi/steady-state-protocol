{
  "name": "@steadystatedefi/protocol-v1",
  "version": "0.9.14",
  "description": "SteadyState smart contracts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/**/*",
    "/dist/*"
  ],
  "author": "Steady State",
  "repository": "https://github.com/steadystatedefi/steady-state-protocol",
  "contributors": [],
  "license": "AGPL-3.0",
  "keywords": [
    "sst",
    "RISQ",
    "ethereum",
    "solidity"
  ],
  "engines": {
    "node": "^16 || ^18"
  },
  "scripts": {
    "run-env": "npm i && tail -f /dev/null",
    "build:package": "rm -rf ./dist && tsc -p tsconfig.build.json && cp types/*.d.ts dist/types && cp -R artifacts dist && cp -R contracts dist",
    "ci:test": "npm run compile && npm run test",
    "ci:clean": "rm -rf ./artifacts ./cache ./types ./dist ./coverage ./storageLayout",
    "eslint": "eslint . --ext=ts,js",
    "eslint:fix": "npm run eslint -- --fix",
    "solhint": "solhint 'contracts/**/*.sol'",
    "sizes": "TS_NODE_TRANSPILE_ONLY=1 SKIP_LOAD=true hardhat compile && TS_NODE_TRANSPILE_ONLY=1 SKIP_LOAD=true hardhat size-contracts",
    "storage": "hardhat storage-layout",
    "console:fork": "FORK=main hardhat console",
    "hardhat": "hardhat",
    "hardhat:kovan": "hardhat --network kovan",
    "hardhat:goerli": "hardhat --network goerli",
    "hardhat:rinkeby": "hardhat --network rinkeby",
    "hardhat:tenderly-main": "hardhat --network tenderlyMain",
    "hardhat:ropsten": "hardhat --network ropsten",
    "hardhat:main": "hardhat --network main",
    "hardhat:mumbai": "KEY_SEL=POL hardhat --network mumbai",
    "hardhat:matic": "KEY_SEL=POL hardhat --network matic",
    "hardhat:bsc-test": "KEY_SEL=BSC hardhat --network bsc_testnet",
    "hardhat:bsc": "KEY_SEL=BSC hardhat --network bsc",
    "hardhat:bsc:fork": "KEY_SEL=BSC_FORK hardhat --network bsc_fork",
    "hardhat:avalanche:fork": "KEY_SEL=AVALANCHE_FORK hardhat --network avalanche_fork",
    "hardhat:avalanche-test:fork": "KEY_SEL=AVALANCHE_TESTNET_FORK hardhat --network avalanche_testnet_fork",
    "hardhat:avalanche": "KEY_SEL=AVA hardhat --network avalanche",
    "hardhat:ava-test": "KEY_SEL=AVA hardhat --network avalanche_testnet",
    "hardhat:fantom": "KEY_SEL=FNT hardhat --network fantom",
    "hardhat:fnt-test": "KEY_SEL=FNT hardhat --network fantom_testnet",
    "hardhat:arbitrum": "KEY_SEL=ARB hardhat --network arbitrum",
    "hardhat:arb-test": "KEY_SEL=ARB hardhat --network arbitrum_testnet",
    "hardhat:optimistic": "KEY_SEL=OPT hardhat --network optimistic",
    "hardhat:opt-test": "KEY_SEL=OPT hardhat --network optimistic_testnet",
    "test-deploy:hardhat": "hardhat deploy-full",
    "test-deploy:hardhat:incremental": "hardhat deploy:test-incremental",
    "test-deploy:fork": "FORK=main hardhat deploy-full",
    "test-deploy:fork:incremental": "FORK=main hardhat deploy:test-incremental",
    "deploy:fork": "npm run compile && FORK=main hardhat deploy-full",
    "deploy:fork:bsc-test": "npm run compile && FORK=bsc_testnet hardhat deploy-full",
    "deploy:fork:bsc": "npm run compile && FORK=bsc hardhat deploy-full",
    "deploy:fork:avalanche": "npm run compile && FORK=avalanche hardhat deploy-full",
    "deploy:fork:avalanche-test": "npm run compile && FORK=avalanche_testnet hardhat deploy-full",
    "deploy:main": "npm run compile && npm run hardhat:main -- deploy-full",
    "deploy:ropsten": "npm run compile && npm run hardhat:ropsten -- deploy-full",
    "deploy:kovan": "npm run compile && npm run hardhat:kovan -- deploy-full",
    "deploy:goerli": "npm run compile && npm run hardhat:goerli -- deploy-full",
    "deploy:rinkeby": "npm run compile && npm run hardhat:rinkeby -- deploy-full",
    "deploy:bsc": "npm run compile && npm run hardhat:bsc -- deploy-full",
    "deploy:bsc-test": "npm run compile && npm run hardhat:bsc-test -- deploy-full",
    "deploy:avalanche": "npm run compile && npm run hardhat:avalanche -- deploy-full",
    "deploy:ava-test": "npm run compile && npm run hardhat:ava-test -- deploy-full",
    "deploy:fantom": "npm run compile && npm run hardhat:fantom -- deploy-full",
    "deploy:fnt-test": "npm run compile && npm run hardhat:fnt-test -- deploy-full",
    "deploy:arbitrum": "npm run compile && npm run hardhat:arbitrum -- deploy-full",
    "deploy:arb-test": "npm run compile && npm run hardhat:arb-test -- deploy-full",
    "deploy:optimistic": "npm run compile && npm run hardhat:optimistic -- deploy-full",
    "deploy:opt-test": "npm run compile && npm run hardhat:opt-test -- deploy-full",
    "deploy:matic": "npm run compile && npm run hardhat:matic -- deploy-full",
    "deploy:mumbai": "npm run compile && npm run hardhat:mumbai -- deploy-full",
    "verify:main": "npm run hardhat:main -- verify:all-contracts",
    "verify:ropsten": "npm run hardhat:ropsten -- verify:all-contracts",
    "verify:kovan": "npm run hardhat:kovan -- verify:all-contracts",
    "verify:goerli": "npm run hardhat:goerli -- verify:all-contracts",
    "verify:rinkeby": "npm run hardhat:rinkeby -- verify:all-contracts",
    "verify:bsc": "npm run hardhat:bsc -- verify:all-contracts",
    "verify:bsc-test": "npm run hardhat:bsc-test -- verify:all-contracts",
    "verify:avalanche": "npm run hardhat:avalanche -- verify:all-contracts",
    "verify:ava-test": "npm run hardhat:ava-test -- verify:all-contracts",
    "verify:fantom": "npm run hardhat:fantom -- verify:all-contracts",
    "verify:fnt-test": "npm run hardhat:fnt-test -- verify:all-contracts",
    "verify:arbitrum": "npm run hardhat:arbitrum -- verify:all-contracts",
    "verify:arb-test": "npm run hardhat:arb-test -- verify:all-contracts",
    "verify:optimistic": "npm run hardhat:optimistic -- verify:all-contracts",
    "verify:opt-test": "npm run hardhat:opt-test -- verify:all-contracts",
    "verify:matic": "npm run hardhat:matic -- verify:all-contracts",
    "verify:mumbai": "npm run hardhat:mumbai -- verify:all-contracts",
    "compile": "SKIP_LOAD=true TS_NODE_TRANSPILE_ONLY=1 hardhat compile",
    "test": "hardhat test ./test-suites/test-main/*.spec.ts ./test-suites/test-main/*.specs/*.spec.ts",
    "test-weighted": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/weighted-rounds.cov.spec.ts",
    "test-join": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/pool-join.cov.spec.ts",
    "test-cancel": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/coverage-cancel.cov.spec.ts",
    "test-imperpetual": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/imperpetual-pool*.cov.spec.ts",
    "test-currency": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/currency.cov.spec.ts",
    "test-balancer": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/balancer.cov.spec.ts",
    "test-premium": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/premium-fund.cov.spec.ts",
    "test-collateral": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/collateral-fund.cov.spec.ts",
    "test-libs": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/lib*.cov.spec.ts",
    "test-access": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/access-control.cov.spec.ts",
    "test-approval": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/approval-catalog.cov.spec.ts",
    "test-pricing": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/pricing.cov.spec.ts",
    "test-yield": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/yield-distributor.cov.spec.ts",
    "test-mcd": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/mcd.cov.spec.ts",
    "test-reinvest": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/reinvestment.cov.spec.ts",
    "test-layout": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/storage-layout.spec.ts",
    "test-access-checks": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/access-checks.specs/*.spec.ts",
    "bench-weighted": "hardhat test test-suites/test-main/__setup.cov.spec.ts test-suites/test-main/weighted-pool.bench.ts",
    "coverage": "TS_NODE_TRANSPILE_ONLY=1 SKIP_LOAD=true hardhat compile --force && TS_NODE_TRANSPILE_ONLY=1 SKIP_LOAD=true hardhat coverage --network coverage --testfiles \"test-suites/test-main/{,*.specs/}*.cov.spec.ts\" ",
    "prettier:check": "npm prettier -c 'helpers/**/*.ts' 'tasks/**/*.ts' 'contracts/**/*.sol' 'test-suites/**/*.ts'",
    "prettier:write": "prettier --write 'helpers/**/*.ts' 'tasks/**/*.ts' 'contracts/**/*.sol' 'test-suites/**/*.ts'",
    "tsc": "tsc -p tsconfig.eslint.json",
    "githook:precommit": "lint-staged && npm run tsc",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.3.0",
    "@tenderly/hardhat-tenderly": "^1.1.0-beta.4",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/lowdb": "^1.0.11",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.6",
    "chai-bignumber": "3.0.0",
    "console-table-printer": "2.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.7.0",
    "hardhat": "^2.10.2",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-tracer": "^1.1.0-rc.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "lowdb": "^1.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "pretty-quick": "^3.1.3",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  }
}
